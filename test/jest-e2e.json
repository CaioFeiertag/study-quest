{
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/$1"
  },
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../src",
  "testEnvironment": "node",
  "testRegex": ".*\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFiles": ["<rootDir>../jest/config/dotenv-config.ts"]
}
